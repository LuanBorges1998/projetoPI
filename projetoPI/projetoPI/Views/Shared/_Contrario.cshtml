@model IEnumerable<projetoPI.Models.contrario>

<p>
    @Html.ActionLink("Adicionar Contrário", "AdicionaContrario", "SocioColaborador", null, new { @class = "links" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nome)
            </td>
            <td>
                @Html.ActionLink("Editar", "EditContrario", new { id = item.id }, new { @class = "links"}) |
                @Html.ActionLink("Excluir", "DeleteContrario", new { id = item.id }, new { @class = "links" })
            </td>
        </tr>
    }

</table>
